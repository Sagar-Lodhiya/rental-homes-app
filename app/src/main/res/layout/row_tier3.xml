<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <com.rentalhomes.utils.customviews.BoldTextView
            android:id="@+id/tvItemName"
            android:layout_width="@dimen/_140sdp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="-5dp"
            android:background="@color/white"
            android:gravity="left"
            android:singleLine="true"
            android:maxLines="1"
            android:ems="10"
            android:maxEms="10"
            android:paddingVertical="@dimen/_10sdp"
            android:paddingStart="@dimen/_15sdp"
            android:paddingEnd="@dimen/_40sdp"
            android:text="Rating Score"
            android:textColor="@color/black"
            android:textSize="@dimen/_12sdp"
            app:layout_constraintEnd_toStartOf="@id/clImage1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clImage1"
            android:layout_width="@dimen/_54sdp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-12dp"
            android:background="@drawable/bg_bottom_shadow"
            android:elevation="@dimen/_20sdp"
            android:paddingTop="@dimen/_2sdp"
            app:layout_constraintEnd_toStartOf="@id/clImage2"
            app:layout_constraintTop_toTopOf="@id/tvItemName">

            <com.rentalhomes.utils.customviews.RegularTextView
                android:id="@+id/tvRowOneLike"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5sdp"
                android:drawableTop="@drawable/ic_like"
                android:drawablePadding="@dimen/_3sdp"
                android:drawableTint="@color/color_green"
                android:paddingStart="@dimen/_3sdp"
                android:text="40"
                android:textColor="@color/color_green"
                android:textSize="@dimen/_10sdp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.rentalhomes.utils.customviews.RegularTextView
                android:id="@+id/tvRowOneLikePercentage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingEnd="@dimen/_2sdp"
                android:text="80%"
                android:textColor="@color/grey_600"
                android:textSize="@dimen/_10sdp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="@id/tvRowOneLike"
                app:layout_constraintEnd_toEndOf="parent" />

            <View
                android:id="@+id/view1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginHorizontal="@dimen/_2sdp"
                android:layout_marginTop="@dimen/_2sdp"
                android:background="#D5D5D5"
                app:layout_constraintEnd_toEndOf="@id/tvRowOneLikePercentage"
                app:layout_constraintStart_toStartOf="@id/tvRowOneLike"
                app:layout_constraintTop_toBottomOf="@id/tvRowOneLikePercentage" />

            <com.rentalhomes.utils.customviews.RegularTextView
                android:id="@+id/tvRowOneDisLike"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5sdp"
                android:drawableTop="@drawable/ic_dislike"
                android:drawablePadding="@dimen/_3sdp"
                android:drawableTint="@color/red_101"
                android:paddingStart="@dimen/_3sdp"
                android:text="10"
                android:textColor="@color/red"
                android:textSize="@dimen/_10sdp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/view1" />

            <com.rentalhomes.utils.customviews.RegularTextView
                android:id="@+id/tvRowOneDisLikePercentage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingEnd="@dimen/_2sdp"
                android:text="20%"
                android:textColor="@color/grey_600"
                android:textSize="@dimen/_10sdp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="@id/tvRowOneDisLike"
                app:layout_constraintEnd_toEndOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clImage2"
            android:layout_width="@dimen/_54sdp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-5dp"
            android:background="@drawable/bg_bottom_shadow"
            android:elevation="@dimen/_20sdp"
            android:paddingTop="@dimen/_2sdp"
            android:visibility="visible"
            app:layout_constraintEnd_toStartOf="@id/clImage3"
            app:layout_constraintTop_toTopOf="@id/tvItemName">

            <com.rentalhomes.utils.customviews.RegularTextView
                android:id="@+id/tvRowTwoLike"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5sdp"
                android:drawableTop="@drawable/ic_like"
                android:drawablePadding="@dimen/_3sdp"
                android:drawableTint="@color/color_green"
                android:paddingStart="@dimen/_3sdp"
                android:text="40"
                android:textColor="@color/color_green"
                android:textSize="@dimen/_10sdp"
                android:visibility="visible"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.rentalhomes.utils.customviews.RegularTextView
                android:id="@+id/tvRowTwoLikePercentage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingEnd="@dimen/_2sdp"
                android:text="80%"
                android:textColor="@color/grey_600"
                android:textSize="@dimen/_10sdp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="@id/tvRowTwoLike"
                app:layout_constraintEnd_toEndOf="parent" />

            <View
                android:id="@+id/view2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginHorizontal="@dimen/_2sdp"
                android:layout_marginTop="@dimen/_2sdp"
                android:background="#D5D5D5"
                app:layout_constraintEnd_toEndOf="@id/tvRowTwoLikePercentage"
                app:layout_constraintStart_toStartOf="@id/tvRowTwoLike"
                app:layout_constraintTop_toBottomOf="@id/tvRowTwoLikePercentage" />

            <com.rentalhomes.utils.customviews.RegularTextView
                android:id="@+id/tvRowTwoDisLike"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5sdp"
                android:drawableTop="@drawable/ic_dislike"
                android:drawablePadding="@dimen/_3sdp"
                android:drawableTint="@color/red_101"
                android:paddingStart="@dimen/_3sdp"
                android:text="10"
                android:textColor="@color/red"
                android:textSize="@dimen/_10sdp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/view2" />

            <com.rentalhomes.utils.customviews.RegularTextView
                android:id="@+id/tvRowTwoDisLikePercentage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingEnd="@dimen/_2sdp"
                android:text="20%"
                android:textColor="@color/grey_600"
                android:textSize="@dimen/_10sdp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="@id/tvRowTwoDisLike"
                app:layout_constraintEnd_toEndOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clImage3"
            android:layout_width="@dimen/_54sdp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-5dp"
            android:background="@drawable/bg_bottom_shadow"
            android:elevation="@dimen/_20sdp"
            android:paddingTop="@dimen/_2sdp"
            android:visibility="invisible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@id/tvItemName">

            <com.rentalhomes.utils.customviews.RegularTextView
                android:id="@+id/tvRowThreeLike"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5sdp"
                android:drawableTop="@drawable/ic_like"
                android:drawablePadding="@dimen/_3sdp"
                android:drawableTint="@color/color_green"
                android:paddingStart="@dimen/_3sdp"
                android:text="0"
                android:textColor="@color/color_green"
                android:textSize="@dimen/_10sdp"
                android:visibility="visible"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.rentalhomes.utils.customviews.RegularTextView
                android:id="@+id/tvRowThreeLikePercentage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingEnd="@dimen/_2sdp"
                android:text="0%"
                android:textColor="@color/grey_600"
                android:textSize="@dimen/_10sdp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="@id/tvRowThreeLike"
                app:layout_constraintEnd_toEndOf="parent" />

            <View
                android:id="@+id/view3"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginHorizontal="@dimen/_2sdp"
                android:layout_marginTop="@dimen/_2sdp"
                android:background="#D5D5D5"
                app:layout_constraintEnd_toEndOf="@id/tvRowThreeLikePercentage"
                app:layout_constraintStart_toStartOf="@id/tvRowThreeLike"
                app:layout_constraintTop_toBottomOf="@id/tvRowThreeLikePercentage" />

            <com.rentalhomes.utils.customviews.RegularTextView
                android:id="@+id/tvRowThreeDisLike"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5sdp"
                android:drawableTop="@drawable/ic_dislike"
                android:drawablePadding="@dimen/_3sdp"
                android:drawableTint="@color/red_101"
                android:paddingStart="@dimen/_3sdp"
                android:text="0"
                android:textColor="@color/red"
                android:textSize="@dimen/_10sdp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/view3" />

            <com.rentalhomes.utils.customviews.RegularTextView
                android:id="@+id/tvRowThreeDisLikePercentage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingEnd="@dimen/_2sdp"
                android:text="0%"
                android:textColor="@color/grey_600"
                android:textSize="@dimen/_10sdp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="@id/tvRowThreeDisLike"
                app:layout_constraintEnd_toEndOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>